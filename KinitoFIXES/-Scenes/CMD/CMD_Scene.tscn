script/source = "extends Node2D
onready var list = $Active/COMMANDS_LIST/LIST
onready var textlab = preload(\"res://KinitoFIXES/-Scenes/CMD/CMD_Text.tscn\")
onready var enterlab = preload(\"res://KinitoFIXES/-Scenes/CMD/CMD_ENTER.tscn\")
func _ready():
	$SOUNDS/AMBIENCE.play()

func create_text(string):
	var clone = textlab.instance()
	clone.get_node(\"Text\").text = string
	list.add_child(clone)
func create_enter():
	var clone = enterlab.instance()
	list.add_child(clone)
	return clone
"

[sub_resource type="GDScript" id=2]
script/source = "extends Button

var pos = Vector2()
var mousePos = Vector2()
var selected = false
func _process(delta):
	mousePos = get_viewport().get_mouse_position()
	if selected == true:
		 get_parent().get_parent().position = mousePos - pos

func _button_down():
	pos = get_viewport().get_mouse_position() - get_parent().get_parent().position
	selected = true

func _button_up():
	selected = false
"

