[sub_resource type="GDScript" id=6]
script/source = "extends Node
onready var tween = get_node(\"MOVE\")
export var SHAKE_VAL : int = 0
export var SHAKE_RATE : float = 20.0
func shake():
	var finish = false
	print('START')
	for numb in range(11 * SHAKE_RATE):
		SHAKE_VAL += 1.0 / SHAKE_RATE
		var curr_pos = get_parent().position
		var offset_x = rand_range(-SHAKE_VAL,SHAKE_VAL)
		var offset_y = rand_range(-SHAKE_VAL,SHAKE_VAL)
		get_parent().position = Vector2(curr_pos.x + offset_x,curr_pos.y + offset_y)
		yield(get_tree().create_timer(1.0 / SHAKE_RATE,false),\"timeout\")
	print('OVER')
"

